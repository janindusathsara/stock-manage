<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="root" prefHeight="500.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.controller.DashboardController">
   <children>
      <Label layoutX="286.0" layoutY="36.0" text="Dashboard" textFill="#e81a1a">
         <font>
            <Font name="System Bold" size="25.0" />
         </font>
      </Label>
      <Button layoutX="14.0" layoutY="185.0" mnemonicParsing="false" onAction="#btnCustomerOnAction" prefHeight="27.0" prefWidth="143.0" style="-fx-background-color: #55E6C1;" text="Customer">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
         <graphic>
            <ImageView fitHeight="20.0" fitWidth="18.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/customer.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button layoutX="14.0" layoutY="270.0" mnemonicParsing="false" onAction="#btnItemOnAction" prefHeight="27.0" prefWidth="143.0" style="-fx-background-color: #25CCF7;" text="Item">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
         <graphic>
            <ImageView fitHeight="18.0" fitWidth="18.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/icons8-item-30.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button layoutX="14.0" layoutY="361.0" mnemonicParsing="false" onAction="#btnSupplierOnAction" prefHeight="28.0" prefWidth="143.0" style="-fx-background-color: #D6A2E8;" text="Supplier">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
         <graphic>
            <ImageView fitHeight="20.0" fitWidth="21.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/icons8-supplier-64.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button layoutX="14.0" layoutY="95.0" mnemonicParsing="false" onAction="#btnDashboardOnAction" prefHeight="27.0" prefWidth="143.0" style="-fx-background-color: #f39c12;" text="Dashboard">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Button>
      <AnchorPane fx:id="node" layoutX="176.0" layoutY="95.0" prefHeight="391.0" prefWidth="510.0" style="-fx-background-color: #c7ecee;">
         <children>
            <TableView layoutX="17.0" layoutY="16.0" prefHeight="358.0" prefWidth="479.0">
              <columns>
                <TableColumn prefWidth="75.0" text="Date" />
                <TableColumn prefWidth="75.0" text="Time" />
                  <TableColumn prefWidth="342.0" text="Venue" />
              </columns>
            </TableView>
         </children>
      </AnchorPane>
      <Button layoutX="14.0" layoutY="448.0" mnemonicParsing="false" onAction="#btnLogoutOnAction" prefHeight="27.0" prefWidth="143.0" style="-fx-background-color: #e74c3c;" text="Logout">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
   </children>
</AnchorPane>
