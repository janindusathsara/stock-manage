<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane fx:id="log" prefHeight="470.0" prefWidth="434.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.controller.LoginFormController">
   <children>
      <Label layoutX="171.0" layoutY="31.0" text="Login">
         <font>
            <Font name="System Bold" size="35.0" />
         </font>
      </Label>
      <TextField fx:id="name" layoutX="55.0" layoutY="163.0" prefHeight="25.0" prefWidth="325.0" promptText="User Name" />
      <TextField fx:id="pass" layoutX="55.0" layoutY="213.0" prefHeight="25.0" prefWidth="325.0" promptText="Password" />
      <Button layoutX="55.0" layoutY="286.0" mnemonicParsing="false" onAction="#btnLoginOnAction" prefHeight="25.0" prefWidth="325.0" style="-fx-background-color: #74b9ff;" text="Login" textFill="#e12d0e">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Label layoutX="132.0" layoutY="344.0" prefHeight="25.0" prefWidth="133.0" text="Don't have an account? " />
      <Label layoutX="265.0" layoutY="348.0" onMouseClicked="#txtRegisterOnClicked" text="Register here" textFill="#161fbf">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Label>
   </children>
</AnchorPane>
